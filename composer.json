{
    "name": "eme-marketing/landing-template",
    "description": "Un template generico para hacer landings con CI/CD para GitLab incluido",
    "type": "project",
    "authors": [
        {
            "name": "Camilo Manrique",
            "email": "camilomanriquec@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/database": "~5.1",
        "illuminate/validation": "~5.1",
        "illuminate/translation": "~5.1",
        "illuminate/filesystem": "~5.1",
        "robmorgan/phinx": "^0.10.7",
        "slim/slim": "^3.12",
        "symfony/dotenv": "3.4.27"
    },
    "autoload":{
        "files": ["php/helpers/url.php", "php/helpers/validation.php"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
